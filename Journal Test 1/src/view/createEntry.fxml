<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox xmlns:fx="http://javafx.com/fxml/1" id="AnchorPane" prefHeight="400.0" prefWidth="600.0" fx:controller="controller.CreateEntryController" spacing = "10" alignment = "CENTER">
    <Label id = "title" text = "Create Entry"/>
    <HBox alignment = "CENTER">
        <Label text = "Name of Entry: " />
        <TextField fx:id="entryName" />
    </HBox>
    <HBox alignment = "CENTER">
        <Label text = "Contents: "/>
        <TextArea fx:id="entryContents" />
    </HBox>
    <Button defaultButton="true" onAction="#handleCreateEntry" text = "Create" /> 
    <Hyperlink fx:id="Back" layoutY="24.0" onAction= "#processBack" text="Back"/>
    <stylesheets>
    <URL value="@Login.css" />
    </stylesheets>
</VBox>
